{
	"name": "Folder",
	"script": {
		"type": "manifest",
		"path": "src/doc-types/Folder/index.ts"
	},
	"roles": {
		"owner": {
			"creatorDefault": true,
			"permissions": ["**", "details:*#$..*"]
		},
		"contributor": {
			"default": false,
			"permissions": ["view", "details:read#$..*"]
		},
		"observer": {
			"default": false,
			"permissions": ["view", "details:read#$..*"]
		}
	}
}
