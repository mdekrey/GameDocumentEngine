{
	"name": "MouseGuard-Conflict",
	"script": {
		"type": "manifest",
		"path": "src/documents/mouseguard-conflict/index.ts"
	},
	"roles": {
		"organizer": {
			"creatorDefault": true,
			"permissions": [
				"view",
				"move",
				"details:*#$.name",
				"details:*#$.details.general..*",
				"details:*#$.details..name",
				"details:read#$.details..disposition.*",
				"details:read#$.details..ready",
				"details:read#$.details..revealed.*"
			]
		},
		"omniscient": {
			"permissions": ["view", "details:read#$..*"]
		},
		"observer": {
			"permissions": [
				"view",
				"details:read#$.name",
				"details:read#$.details.general..*",
				"details:read#$.details..name",
				"details:read#$.details..disposition.*",
				"details:read#$.details..ready",
				"details:read#$.details..revealed.*"
			]
		},

		"side-a-captain": {
			"permissions": [
				"view",
				"details:read#$.name",
				"details:read#$.details.general..*",
				"details:*#$.details.sideA",
				"details:*#$.details.sideA..*",
				"details:read#$.details..name",
				"details:read#$.details..disposition.*",
				"details:read#$.details..ready",
				"details:read#$.details..revealed.*"
			]
		},
		"side-a-member": {
			"permissions": [
				"view",
				"details:read#$.name",
				"details:read#$.details.general..*",
				"details:read#$.details.sideA..*",
				"details:read#$.details..name",
				"details:read#$.details..disposition.*",
				"details:read#$.details..ready",
				"details:read#$.details..revealed.*"
			]
		},
		"side-b-captain": {
			"permissions": [
				"view",
				"details:read#$.name",
				"details:read#$.details.general..*",
				"details:*#$.details.sideB",
				"details:*#$.details.sideB..*",
				"details:read#$.details..name",
				"details:read#$.details..disposition.*",
				"details:read#$.details..ready",
				"details:read#$.details..revealed.*"
			]
		},
		"side-b-member": {
			"permissions": [
				"view",
				"details:read#$.name",
				"details:read#$.details.general..*",
				"details:read#$.details.sideB..*",
				"details:read#$.details..name",
				"details:read#$.details..disposition.*",
				"details:read#$.details..ready",
				"details:read#$.details..revealed.*"
			]
		}
	}
}
